#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n'
int main()
{
    optimize();

    int t,a,b,sum;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        cin>>a>>b;
        sum=a+b;
        cout<<"Case "<<i+1<<": "<<sum<<endl;
    }
    return 0;
}
