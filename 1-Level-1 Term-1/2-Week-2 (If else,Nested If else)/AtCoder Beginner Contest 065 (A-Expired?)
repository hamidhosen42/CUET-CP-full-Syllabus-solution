#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n'
int main()
{
    optimize();

    int a,x,b;
    cin>>x>>a>>b;

    if(a>=b) cout<<"delicious"<<endl;
    else if(x>=(b-a)) cout<<"safe"<<endl;
    else cout<<"dangerous"<<endl;
    return 0;
}
