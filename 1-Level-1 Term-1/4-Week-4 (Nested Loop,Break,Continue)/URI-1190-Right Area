#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n'
#define ll long long int
int main()
{
    optimize();

    string s;
    double array[12][12],sum=0.0;
    cin>>s;
    for(int i=0;i<12;i++)
    {
        for(int j=0;j<12;j++)
        {
            cin>>array[i][j];
        }
    }
    for(int i=11;i>=7;i--)
    {
        for(int j=12-i;j<=i-1;j++)
        {
            sum+=array[j][i];
        }
    }
    if(s[0]=='S')
    {
        cout<<fixed;
        cout<<setprecision(1)<<sum<<endl;
    }
    if(s[0]=='M')
    {
        cout<<fixed;
        cout<<setprecision(1)<<sum/30.0<<endl;
    }
    return 0;
}
